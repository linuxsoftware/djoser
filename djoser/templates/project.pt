<html metal:use-macro="load: main.pt">
      <title metal:fill-slot="title">Project</title>
      <a metal:fill-slot="module-title" href="/projects" title="Contacts">
          <div class="module-title">
              Projects
          </div>
      </a>
      <div metal:fill-slot="content" id="content">
        <div id="projects">
            <div class="project">
              <div class="field" tal:repeat="field form">
                ${field.label} ${field.data}
              </div>
            </div> <!-- project -->
            <div class="tool-bar">
            </div>
        </div>
      </div>
</html>
